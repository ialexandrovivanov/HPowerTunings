@using HPowerTunings.ViewModels.EmployeeModels
@model ICollection<EmployeeStartEndViewModel>

@{
    ViewData["Title"] = "EmployeeStatistics";
    Layout = "~/Views/Shared/_LayoutBoss.cshtml";
}
<p></p>
<p></p>
<h4 class="mt-5 mb-5">EmployeeStatistics</h4>
@foreach (var item in Model)
{
    <div class="container-fluid justify-content-center text-center" 
         style="max-width:600px; background-color:cadetblue; margin-top:10px; 
         margin-bottom:10px; margin-left:10px; margin-right:10px; border-radius:6px">
        <p>@item.FullName</p>
        <p>@item.Possition</p>
        <p>@item.Email</p>
        <p>@item.PhoneNumber</p>
    </div>

    <p></p>
}
